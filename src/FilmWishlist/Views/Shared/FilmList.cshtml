@model FilmWishlist.Models.FilmListViewModel

<table class="table table-striped table-bordered">
  <thead>
  <tr>
    <th>Title</th>
    <th>Year</th>
  </tr>
  </thead>
  @foreach (var film in Model.Films)
  {
      <tr>
        <td>
          @film.Title
        </td>
        <td>
          @film.Year
        </td>
      </tr>
  }
</table>
